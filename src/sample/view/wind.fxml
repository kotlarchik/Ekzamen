<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<Pane maxHeight="-Infinity" maxWidth="-Infinity"
      minHeight="-Infinity" minWidth="-Infinity"
      prefHeight="200.0" prefWidth="400.0"
      xmlns="http://javafx.com/javafx/11.0.1"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controllers.ControllerWind">
   <children>
      <Label layoutX="149.0" layoutY="14.0" text="Внимание!" textFill="RED">
         <font>
            <Font name="Trebuchet MS Bold" size="20.0" />
         </font>
      </Label>
      <Label alignment="CENTER" layoutX="8.0" layoutY="58.0" prefHeight="56.0" prefWidth="386.0" text="Все несохранённые данные будут утеряны." wrapText="true">
         <font>
            <Font name="Trebuchet MS" size="18.0" />
         </font>
      </Label>
      <Button fx:id="buttonExit" layoutX="60.0" layoutY="140.0" mnemonicParsing="false" onAction="#pressExit" text="Выйти">
         <font>
            <Font name="Trebuchet MS Bold" size="20.0" />
         </font>
      </Button>
      <Button fx:id="buttonBack" layoutX="253.0" layoutY="140.0" mnemonicParsing="false" onAction="#pressBack" text="Остаться">
         <font>
            <Font name="Trebuchet MS Bold" size="20.0" />
         </font>
      </Button>
   </children>
</Pane>
